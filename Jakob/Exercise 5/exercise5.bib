  keywords = {Cirrhosis,Costs,HRQOL,NAFLD,Population},
  file = {/Users/noltinho/Zotero/storage/MG6YIPS6/Stepanova et al. - 2017 - Direct and Indirect Economic Burden of Chronic Liv.pdf}
}

