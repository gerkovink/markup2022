@book{Hemenway_2005, address={New York}, title={Divine proportion: Phi in art, nature, and science}, ISBN={9781402735226}, abstractNote={The number Phi, simply defined, is one plus the square root of five, all divided by two. But its myriad occurrences in art, nature, and science have been a source of speculation and wonder for thousands of years. "Divine Proportion "draws upon both religion and science to tell the story of Phi and to explore its manifestations in such diverse places as the structure of the inner ear, the spiral of a hurricane, the majesty of the Parthenon, and the elusive perfection of the “Mona Lisa.” A}, publisher={Published by Sterling Pub. Co.}, author={Hemenway, Priya}, year={2005}, language={eng} }